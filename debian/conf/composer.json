{
    "name": "vitexsoftware/icinga-editor",
    "description": "Editor of Icinga Configuration files (debianized)",
    "version": "1.2.1.2",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "vendor-dir": "/var/lib/icinga-editor"
    },
    "require": {
        "php": ">= 5.5.9",
        "ease-framework": "*",
        "ease-bricks": "*",
        "league/commonmark": "^0.15.0",
        "pear/mail": ">=1.3.0",
        "pear/mail_mime": ">=1.10.0"
    },
    "autoload": {
        "psr-4": {
            "Icinga\\Editor\\": "classes/",
            "Icinga\\Editor\\Engine\\": "classes/engine/",
            "Icinga\\Editor\\UI\\": "classes/UI",
            "Icinga\\Editor\\modules\\": "modules"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease"
        },
        {
            "type": "path",
            "url": "/usr/share/php/EaseBricks"
        }
    ]

}
